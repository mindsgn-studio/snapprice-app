{
  "expo": {
    "name": "Snap Price",
    "slug": "snapprice-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "snapprice.mindsgn.studio",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f4511e"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 2, 
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#f4511e"
      },
      "package": "price.mindsgn.studio"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d713b743-fc79-4c78-a6a7-9826f63bb332"
      }
    }
  }
}
